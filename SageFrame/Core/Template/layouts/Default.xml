<?xml version="1.0" encoding="utf-8"?>
<layout name="layout_all">
  <section name="sfHeader">
    <placeholder name="headers" width="30" class="header">logo,search</placeholder>
    <placeholder name="navigation">navigation</placeholder>
    <placeholder name="message">message</placeholder>
    <placeholder name="banner">banner</placeholder>
    <placeholder name="fixed" mode="fixed">design,developer,seo</placeholder>
    <placeholder name="fulltopspan">getstarted</placeholder>
  </section>
  <section name="sfContent">
    <placeholder name="RightB" width="25">RightB</placeholder>
    <placeholder name="middlemainbottom" mode="fixed">KeyFeatures,NewFeatures</placeholder>
  </section>
  <section name="sfFooter">
    <placeholder name="footerBlocks" mode="fixed">testimonials,subscribe,social,contact</placeholder>
    <placeholder name="copyright">copyright</placeholder>
  </section>
  <wrappers>
    <wrap type="block">sfHeader,sfContent</wrap>
    <wrap type="placeholder" class="sfFooterWrapper">footerblocks,copyright</wrap>
  </wrappers>
</layout>